import java.util.*;

public class table {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number for table");
        int n=sc.nextInt();
        int i=1;
        int multi=1;
        while(i<=11){
            System.out.println(n+"*"+i+"="+n*i);
            i++;
        }
    }
}
